<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.webServer>
        <staticContent>
            <mimeMap fileExtension=".md" mimeType="text/markdown" />
        </staticContent>

        <httpProtocol>
            <customHeaders>
                <add name="Strict-Transport-Security" value="max-age=63072000" />
                <add name="Content-Type-Options" value="nosniff" />
                <add name="X-Frame-Options" value="SAMEORIGIN" />
                <add name="Content-Security-Policy" value="Content-Security-Policy: default-src 'self'" />
                <add name="X-XSS-Protection" value="1; mode=block" />
            </customHeaders>
        </httpProtocol> 

    </system.webServer>
</configuration>
